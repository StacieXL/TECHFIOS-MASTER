package topics;
import java.util.ArrayList;
import java.util.HashSet;
import java.util.LinkedHashSet;
import java.util.Set;
import java.util.TreeSet;
public class Hashsett {
	public static void main(String[] args) {
		
		ArrayList<Integer> list = new ArrayList<>(); 
		list.add(4);
		list.add(5); 
		list.add(6); 
		list.add(9);
		list.add(8);
		list.add(7);
		list.add(10);
		list.add(1);
		list.add(2);
		list.add(3);
		System.out.println("List (as is, add to end): \n"+list);
		System.out.println();
			 	
		Set<Integer> hash = new HashSet<>(); 				
		hash.add(40);
		hash.add(50); 
		hash.add(60); 
		hash.add(90);
		hash.add(80);
		hash.add(70);
		hash.add(100);
		hash.add(10);
		hash.add(20);
		hash.add(30);
		System.out.println("Hash set (random): \n"+hash);
		System.out.println();
		
		Set<String> hashStr = new HashSet<>(); 				
		hashStr.add("b");
		hashStr.add("d"); 
		hashStr.add("e"); 
		hashStr.add("g");
		hashStr.add("c");
		hashStr.add("a");
		hashStr.add("f");
		hashStr.add("i");
		hashStr.add("a");
		hashStr.add("h");
		System.out.println("Hash set (random): \n"+hashStr);
		System.out.println();
		
		Set<Integer> linkedHash = new LinkedHashSet<>(); 				
		linkedHash.add(4);
		linkedHash.add(5); 
		linkedHash.add(6); 
		linkedHash.add(9);
		linkedHash.add(8);
		linkedHash.add(7);
		linkedHash.add(10);
		linkedHash.add(1);
		linkedHash.add(2);
		linkedHash.add(3);
		System.out.println("LinkedHash (as is, add to end): \n"+linkedHash);
		System.out.println();
		
		Set<Integer> tree = new TreeSet<>(); 				
		tree.add(4);
		tree.add(5); 
		tree.add(6); 
		tree.add(9);
		tree.add(8);
		tree.add(7);
		tree.add(10);
		tree.add(1);
		tree.add(2);
		tree.add(3);
		System.out.println("Tree set (sorted): \n"+tree);
		System.out.println();                           
	}
}  