package topics;

import topics.staticNonstatic.E_DifferentClass;

public class GlobalVar{	
	static int c = 3;
	static int d = 4;
	int e = 5;
	int f = 6;
	
	public static void main(String[] args) { 
		int a = 1;
		int b = 2;	
		
		staticSum (a,b);    //local              
		staticSum (c,d);    //global static           
		staticSum ();		//global Non-static (e,f)
		
		GlobalVar object2 = new GlobalVar ();
		object2.nonStatic(a,b);
	}

	public static void staticSum(int num1, int num2) {
		System.out.println(num1 + num2);
	}	
	public static void staticSum() {
		GlobalVar object = new GlobalVar ();                 	
		System.out.println(object.e + object.f);  //global Non-static
	}	
	public void nonStatic(int num1, int num2) {
		System.out.println(num1 + num2);              	
//		System.out.println(object.e + object.f);  //global Non-static
	}	

	
}



	