package Topics;

public class C_ForWhileBasic {
	public static void main(String[] args) {

// 1.print 1,4,8,12,16...                For loop
		int j =5;
		for (j=j; j <11; j++) {
			System.out.print(j+" ");	
		}			
		System.out.println();
		
		
// 2.Given N ----print backwards to 0	while loop
		while (j >= 0) {
			System.out.print(j+" ");
			j--;
		}
		System.out.println();
		
		
		
		do {
		  System.out.println(j);
		  j++;
		}
		while (j <= 5);		
	}
}

