package topics.Pack1;
public class AccessModifiers {
		 static int s = 0;
		private int	a = 1;
				int b = 2;
	  protected int c = 3;
		 public int d = 4;
		 
		 private void sum3() {
			 int x;
		 }
		 public void sum () {
			 System.out.println(10+10);
		 }
		 public static void sumStatic() {
			 System.out.println(10+10);
			 System.out.println(s+20);
		 }
		 
}
