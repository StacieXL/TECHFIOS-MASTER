package topics;

import java.util.ArrayList;
public class Arrays {

	public static void main(String[] args) {

		String [] Colors= {"red", "blue","white"};
		
		java.util.Arrays.sort(Colors);
		
		
		
		for(String item : Colors) {
		
		System.out.println(item);
		
		}
		
		Colors.add("neutro");
		
		
	}

		
	}


