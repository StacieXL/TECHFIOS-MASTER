package topics;

import java.util.ArraysList;
public class Arrays {

	public static void main(String[] args) {
		String [] Colors= {"red", "blue","white"};
		
		java.util.Arrays.sort(Colors);
		
		
		
		for(String item : Colors) {
		
		System.out.println(item);
		
		}
		
		ArrayList<String> Code = new ArrayList<String>(); // Create an ArrayList object
		
			Code.add(0, "null");
	
	
	
	
	}
	
	
	

		
	}


