package homeWorks;
public class HW4Overload2 {

	public HW4Overload2() {          // default no input
		System.out.println("Print default constructor");	
		System.out.println();		
	}	
	public HW4Overload2(String x) {  //single argument
		System.out.println("1 argument String = \"" +x+"\"");
		System.out.println();
	}		
	public HW4Overload2(int x) {
		System.out.println("1 argument int    =  " +x);	
		System.out.println();
	}	
	public HW4Overload2(String x, String y) {  //two argument
		System.out.println("2 args: String + String     = "+  x+" & "+y);	
		System.out.println();
	}	
	public HW4Overload2(int x, String y) {
		System.out.println("2 args: int + String        = "+y+ " #"+x);		
		System.out.println();
	}	
	public HW4Overload2(String y, int x) {
		System.out.println("2 args: (Reverse parameter) = "+y+ " still #"+x);
		System.out.println();
	}	
	public HW4Overload2(String y, int x, int z) {
		System.out.println("3 args: arg1 + arg2 + arg3  = "+y+" "+x+" "+z);			
		System.out.println();
	}
	public void HW4Overload2() {
		System.out.println("Method overload void");			
		System.out.println();
		return;
	}
	public int HW4Overload2(int x, int y) {
		System.out.println("Method overload int");	
		System.out.println();
		return x+y;
	}
	public String HW4Overload2(String y) {
		System.out.println("Method overload String");	
		System.out.println();
		return y;
	}
}
