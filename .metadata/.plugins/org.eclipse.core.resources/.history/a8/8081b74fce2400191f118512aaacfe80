package Topics;

public class Sandbox2 {
	public static void main(String[] args) {
		//For 2D		
				for (int i = 1; i <=4; i++) {
					for (int j = 1; j <=4; j++) {	
						if (j ==2) {continue;}
					System.out.print(i*j+ " ");	
					}	
					System.out.println();
				}	
				System.out.println();
	
	}
}